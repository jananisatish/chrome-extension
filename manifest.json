{
  "manifest_version": 3,
  "name": "Tetris",
  "version": "1.0.1",
  "description": "A Chrome extension where you can play tetris",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {              
      "128": "128.png"      
    },
    "default_title": "Tetris"   
  }
}